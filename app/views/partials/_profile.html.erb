<div class="container py-3 mb-3">
  <div class="row">
    <div class="col-lg-3">
      <div class="card">
        <div class="card-body">
          <ul class="list-group list-group-flush">
            <%= link_to backoffice_profile_path, class: "list-group-item #{active_class_multiple([backoffice_profile_path])}" do %>
              <%= fa_icon "home lg" %><span>&nbsp;</span>Dados Pessoais
            <%- end %>
            <%= link_to backoffice_address_path, class: "list-group-item #{active_class_multiple([backoffice_address_path])}" do %>
              <%= fa_icon "home lg" %><span>&nbsp;</span>Meu Endereço
            <%- end %>
            <%= link_to backoffice_banking_account_path, class: "list-group-item #{active_class_multiple([backoffice_banking_account_path])}" do %>
              <%= fa_icon "home lg" %><span>&nbsp;</span>Dados Bancários
            <%- end %>
            <%= link_to backoffice_profile_path, class: "list-group-item #{active_class_multiple([backoffice_profile_path])}" do %>
              <%= fa_icon "home lg" %><span>&nbsp;</span>Meus Documentos
            <%- end %>
            <%= link_to backoffice_account_password_path, class: "list-group-item #{active_class_multiple([backoffice_account_password_path])}" do %>
              <%= fa_icon "home lg" %><span>&nbsp;</span>Alterar Senha
            <%- end %>
          </ul>
        </div>
      </div>
    </div>
    <div class="col-lg-9">
      <%= yield -%>
    </div>
  </div>
</div>
